{
	"name": "nativescript-stripe-angular",
	"ngPackage": {
		"lib": {
			"entryFile": "index.ts",
			"umdModuleIds": {
				"@nativescript/core": "ns-core",
				"@nativescript/angular": "ns-angular",
				"nativescript-stripe": "ns-stripe"
			}
		},
		"whitelistedNonPeerDependencies": [
			"."
		]
	},
	"scripts": {
		"build": "npm i && ng-packagr -p package.json && cd dist && cpy '**/*' '../../src/angular' --parents",
		"clean": "npx rimraf node_modules dist package-lock.json -- *.js"
	},
	"devDependencies": {
		"@angular/animations": "~10.1.0",
		"@angular/common": "~10.1.0",
		"@angular/compiler": "~10.1.0",
		"@angular/compiler-cli": "~10.1.0",
		"@angular/core": "~10.1.0",
		"@angular/forms": "~10.1.0",
		"@angular/platform-browser": "~10.1.0",
		"@angular/platform-browser-dynamic": "~10.1.0",
		"@angular/router": "~10.1.0",
		"@nativescript/angular": "~10.0.0",
		"@nativescript/core": "~7.0.0",
		"cpy-cli": "~3.1.1",
		"nativescript-stripe": "file:../src",
		"ng-packagr": "~10.0.0",
		"rxjs": "^6.6.0",
		"tslint": "^5.14.0",
		"typescript": "~3.9.0",
		"zone.js": "^0.10.3"
	}
}
